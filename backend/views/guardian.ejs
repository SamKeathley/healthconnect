<% include partials/header %>
    <section>
      <div class="opac">
      <div class="container">
        <div class="row">
            <nav class="nav col-md-4">
              <ul>
                <div class="guardNav"><a href="#"><div class="navHover">Home</div></a></div>
                <div class="guardNav" id="guardDrop"><a href="#"><div class="navHover">Student Profile</div></a></div>
                  <ul class="dropMenu">
                    <div class="subNav"><a href="#"><div class="navHover">One</div></a></div>
                    <div class="subNav"><a href="#"><div class="navHover">Two</div></a></div>
                    <div class="subNav subNavBottom"><a href="#"><div class="navHover">Three</div></a></div>
                  </ul>
                <div class="guardNav guardNavBottom"><a href="#"><div class="navHover">Create New Report</div></a></div>
              </ul>
            </nav>
      <div class="content-container col-md-5">
      <p><img src="<%=image%>"/></p>
      <p class="guardian-profile">Name: <%= name %></p>
      <p class="guardian-profile">DOB: <%= birthdate %></p>
      <p class="guardian-profile">Guardian: <%= guardian %></p>
      <p class="guardian-profile">Guardian Number: <%= guardian_number %></p>
      <p class="guardian-profile">Guardian Email: <%= guardian_email %></p>
      <p class="guardian-profile">Medical History: <%= medicalHistory %></p>
      <p class="guardian-profile">Allergies: <%= allergies %></p>
      <p class="guardian-profile">Immunizations: <%= immunizations %></p>
      <h3>Reports</h3>
      <% for (var i=0, x=reports.length; i<x; i++) { %>
        <p class="guardian-report top">Date: <%= reports[i].date %></p>
        <p class="guardian-report">Vitals: <%= reports[i].vitals %></p>
        <p class="guardian-report">Symptoms: <%= reports[i].symptoms %></p>
        <p class="guardian-report">Notes: <%= reports[i].call_notes %></p>
        <p class="guardian-report bottom">Treatment Plan: <%= reports[i].treatment_plan %></p>
      <% } %>
    </div>
  </div>
  </div>
  </div>
    </section>

    <% include partials/footer %>
